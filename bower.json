{
  "name": "ArnoldC-Speaker",
  "version": "0.0.0",
  "authors": [
    "Cesar Augusto D. Azevedo <cesardeazevedo@outlook.com>"
  ],
  "description": "A bit more emotion for ArnoldC scripts",
  "keywords": [
    "ArnoldC",
    "Speaker",
    "Voice",
    "Narration"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.2.25",
    "bootstrap": "~3.2.0",
    "jquery": "~2.1.1",
    "ace": "~1.1.7",
    "angular-audio": "*"
  },
  "overrides": {
    "angular": {
      "main": "angular.min.js"
    },
    "angular-audio": {
      "main": "angular.audio.js"
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap-theme.min.css",
        "**/glyphicons-halflings-regular.ttf",
        "**/glyphicons-halflings-regular.woff",
        "dist/fonts/glyphicons-halflings-regular.woff"
      ]
    },
    "jquery": {
      "main": "dist/jquery.min.js"
    },
    "ace": {
      "main": "build/src/ace.js"
    }
  }
}
